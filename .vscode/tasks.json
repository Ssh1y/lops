{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc 生成活动文件",
            "command": "/usr/bin/gcc",
            "args": [
                "${workspaceFolder}/verify/VerifyCase1.c",
                "${workspaceFolder}/kernel/base/sched/los_sched.c",
                "${workspaceFolder}/kernel/base/sched/los_priority.c",
                "${workspaceFolder}/kernel/base/sched/los_deadline.c",
                "${workspaceFolder}/kernel/base/core/los_task.c",
                "${workspaceFolder}/kernel/base/sched/los_sortlink.c",
                "-I${workspaceFolder}/kernel/base/include/",
                "-I${workspaceFolder}/kernel/include/",
                "-I${workspaceFolder}/kernel/common/",
                "-I${workspaceFolder}/arch/arm/arm/include",
                "-g",
                "-o",
                "${workspaceFolder}/verify/VerifyCase1",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ]
}